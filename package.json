{
  "name": "witdom-broker-api",
  "version": "1.0.0",
  "description": "API to use services from the Broker",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node broker.js",
    "test": "./node_modules/.bin/mocha tests/nodejs/integration",
    "api_test": "mocha --timeout 10000 tests/nodejs/integration/api.js",
    "db_test": "./node_modules/.bin/mocha --timeout 5000 tests/nodejs/unit/database.js",
    "request_test": "./node_modules/.bin/mocha --timeout 10000 tests/nodejs/unit/requestForwarding.js",
    "unit_test": "./node_modules/.bin/mocha tests/nodejs/unit/dummyTokenValidatorTest.js; ./node_modules/.bin/mocha --timeout 15000 tests/nodejs/unit/iamTokenValidatorTest.js",
    "token_test": "./node_modules/.bin/mocha tests/nodejs/unit/dummyTokenValidatorTest.js; ./node_modules/.bin/mocha --timeout 15000 tests/nodejs/unit/iamTokenValidatorTest.js",
    "si_test": "mocha  --timeout 5000 tests/nodejs/unit/serviceInfoTest.js",
    "mocksi_test": "mocha  tests/nodejs/unit/mockServiceInfoTest.js",
    "build_broker_td": "./broker_docker.sh --command=build-images --dockerfile=Dockerfile_td --image-name=witdom-core-broker-td --container-name=broker_td",
    "rebuild_broker_td": "./broker_docker.sh --command=rebuild-images --dockerfile=Dockerfile_td --image-name=witdom-core-broker-td --container-name=broker_td",
    "build_broker_ud": "./broker_docker.sh --command=build-images --dockerfile=Dockerfile_ud --image-name=witdom-core-broker-ud --container-name=broker_ud",
    "rebuild_broker_ud": "./broker_docker.sh --command=rebuild-images --dockerfile=Dockerfile_ud --image-name=witdom-core-broker-ud --container-name=broker_ud",
    "run_broker_td": "./broker_docker.sh --command=run-containers --image-name=witdom-core-broker-td --container-name=broker_td --mongo-container=mongo-broker-td --container-http-port=5000 --container-https-port=5043 --host-http-port=5000 --host-https-port=5043 --remote-host=10.5.1.120 --remote-http-port=5100 --remote-https-port=5143 --other-domain-name=broker-ud --use-iam=yes --iam-container=iam",
    "run_broker_ud": "./broker_docker.sh --command=run-containers --image-name=witdom-core-broker-ud --container-name=broker_ud --mongo-container=mongo-broker-ud --container-http-port=5000 --container-https-port=5043 --host-http-port=5100 --host-https-port=5143 --remote-host=10.5.1.120 --remote-http-port=5000 --remote-https-port=5043 --other-domain-name=broker-td",
    "delete_images_broker_td": "./broker_docker.sh --command=delete-images --image-name=witdom-core-broker-td --container-name=broker_td",
    "delete_images_broker_ud": "./broker_docker.sh --command=delete-images --image-name=witdom-core-broker-ud --container-name=broker_ud",
    "start_broker_td": "./broker_docker.sh --command=start-containers --container-name=broker_td",
    "stop_broker_td": "./broker_docker.sh --command=stop-containers --container-name=broker_td",
    "remove_broker_td_containers": "./broker_docker.sh --command=remove-containers --container-name=broker_td",
    "start_broker_ud": "./broker_docker.sh --command=start-containers --container-name=broker_ud",
    "stop_broker_ud": "./broker_docker.sh --command=stop-containers --container-name=broker_ud",
    "remove_broker_ud_containers": "./broker_docker.sh --command=remove-containers --container-name=broker_ud",
    "bash_td": "sudo docker exec -it broker_td /bin/bash",
    "bash_ud": "sudo docker exec -it broker_ud /bin/bash",
    "log_td" : "sudo docker logs broker_td",
    "log_ud" : "sudo docker logs broker_ud"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "openstack-wrapper": "mancabizjak/node-openstack-wrapper",
    "openstack-token-utils":"./dependencies/iam/keystone-clients/javascript-wrappers/node-openstack-token-utils",
    "array-includes": "^3.0.2",
    "cloudify_provider_connector": "file:orchestration/cloudify_provider_connector",
    "connect": "^3.2.0",
    "js-yaml": "^3.3.0",
    "lodash": "^4.16.4",
    "mock_example": "file:orchestration/mock_example",
    "mongoose": "^4.6.6",
    "request": "^2.79.0",
    "request-ip": "^1.2.2",
    "swagger-tools": "0.10.1",
    "superagent": "^3.0.0",
    "unirest": "^0.5.1",
    "ursa": "^0.9.4",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "should": "~11.1.1",
    "supertest": ">=3.0.0",
    "istanbul": "~0.4.5",
    "nock": "~8.1.0",
    "mocha": "~3.1.2",
    "mocha-junit-reporter": "~1.12.1",
    "xunit-file": "~1.0.0"
  }
}
