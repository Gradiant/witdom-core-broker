#
# client_ud.cnf
#

[ req ]
default_bits		= 4096
days			= 9999
prompt                  = no
distinguished_name      = client_ud
req_extensions          = v3_req

[ client_ud ]
commonName              = client-ud
stateOrProvinceName     = Pontevedra
countryName             = ES
emailAddress            = client_ud@gradiant.org
organizationName        = Gradiant
organizationalUnitName  = Clients

[ v3_req ]
basicConstraints        = CA:FALSE
keyUsage                = nonRepudiation, digitalSignature, keyEncipherment
