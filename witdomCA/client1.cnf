#
# client1.cnf
#

[ req ]
default_bits		= 4096
days			= 9999
prompt                  = no
distinguished_name      = witdom_client
req_extensions          = v3_req

[ witdom_client ]
commonName              = client1
stateOrProvinceName     = Pontevedra
countryName             = ES
emailAddress            = client1@witdom.eu
organizationName        = Witdom
organizationalUnitName  = Clients

[ v3_req ]
basicConstraints        = CA:FALSE
keyUsage                = nonRepudiation, digitalSignature, keyEncipherment
